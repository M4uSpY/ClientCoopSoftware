<UserControl
    x:Class="ClientCoopSoft.Views.Reportes.ReportesView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ClientCoopSoft.Views.Reportes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid Margin="20">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Button
            Grid.Row="0"
            Grid.Column="0"
            Content="Usuarios" />
        <Button
            Grid.Row="1"
            Grid.Column="0"
            Content="Trabajadores" />
        <Button
            Grid.Row="1"
            Grid.Column="1"
            Content="Personas" />
        <Button
            Grid.Row="0"
            Grid.Column="1"
            Content="Faltas" />
        <Button
            Grid.Row="0"
            Grid.Column="2"
            Content="Registros de Acceso" />
        <Button
            Grid.Row="1"
            Grid.Column="2"
            Content="Planilla de Sueldos y Salarios" />
    </Grid>
</UserControl>
